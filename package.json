{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./snippets/global.json"
      },
      {
        "language": [
          "gitcommit",
          "NeogitCommitMessage"
        ],
        "path": "./snippets/gitcommit.json"
      },
      {
        "language": [
          "shellscript",
          "shell",
          "sh",
          "zsh"
        ],
        "path": "./snippets/shell.json"
      },
      {
        "language": [
          "markdown",
					"vimwiki",
          "rmd"
        ],
        "path": "./snippets/markdown.json"
      },
      {
        "language": [
          "markdown",
          "vimwiki"
        ],
        "path": "./snippets/latex/latex-snippets.json"
      },
      {
        "language": [
          "markdown",
          "vimwiki"
        ],
        "path": "./snippets/latex/vscode-latex-snippets.json"
      },
      {
        "language": [
          "plaintex",
          "tex"
        ],
        "path": "./snippets/latex.json"
      },
      {
        "language": [
          "html",
          "jade",
          "pug",
          "eruby"
        ],
        "path": "./snippets/html.json"
      },
      {
        "language": "python",
        "path": "./snippets/python/python.json"
      },
      {
        "language": "python",
        "path": "./snippets/python/unittest.json"
      },
      {
        "language": [
          "plaintex",
          "tex"
        ],
        "path": "./snippets/latex/latex-snippets.json"
      },
      {
        "language": [
          "plaintex",
          "tex"
        ],
        "path": "./snippets/latex/vscode-latex-snippets.json"
      }
    ]
  }
}
